#cuerpo form {
	padding-left: 30px;
	line-height: 40px;
	font-weight: 600;
	font-style: italic;
}

.nombre {
	width: 300px;
}

#domicilio {
	width: 300px;
}

input[type=text] {
	background-color: #FEFDF9;
	border-radius: 10px;
}

